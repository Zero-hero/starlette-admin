{
  "name": "styles",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "css": "npm-run-all css-compile* --sequential css-prefix* css-minify*",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/admin.scss starlette_admin/statics/starlette_admin/css/admin.css",
    "css-prefix": "postcss --config scss/postcss.config.js --replace \"starlette_admin/statics/starlette_admin/css/*.css\" \"!starlette_admin/statics/starlette_admin/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output starlette_admin/statics/starlette_admin/css/admin.min.css starlette_admin/statics/starlette_admin/css/admin.css",
    "watch-css": "nodemon -e scss -x \"npm run css\"",
    "js-minify": "rm starlette_admin/statics/starlette_admin/js/*.min.js; for f in starlette_admin/statics/starlette_admin/js/*.js; do short=${f%.js}; uglifyjs $f > $short.min.js; done"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "autoprefixer": "^9.5.0",
    "clean-css-cli": "^4.2.1",
    "cpy-cli": "^2.0.0",
    "karma-css": "git+https://git@github.com/accentdesign/karma-css.git",
    "karma-css-components": "git+https://git@github.com/accentdesign/karma-css-components.git",
    "node-sass": "^4.7.2",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "uglify-js": "^3.4.10"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
